{
  "name": "optiprism-browser",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "yarn build && vite",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "generate:openapi": "openapi-generator-cli generate -i openapi.yaml -g typescript-fetch --skip-validate-spec -o ./src/api -c openapi-generator-config.json",
    "express": "node ./src/server/index.cjs",
    "test:unit": "vitest ./src/tests/unit",
    "test:e2e": "playwright test",
    "fmt": "prettier ./src --write",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "typecheck": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "consola": "^3.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@faker-js/faker": "^8.4.1",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@playwright/test": "^1.43.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.12.7",
    "@vitest/coverage-istanbul": "^1.6.0",
    "cors": "^2.8.5",
    "eslint": "^9.2.0",
    "express": "^4.19.2",
    "globals": "^15.2.0",
    "jsdom": "^24.0.0",
    "morgan": "^1.10.0",
    "prettier": "^3.2.5",
    "rollup": "^4.14.1",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.11",
    "vitest": "^1.4.0"
  }
}
