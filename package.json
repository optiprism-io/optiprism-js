{
  "name": "optiprism_analytics_browser",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start-test-server": "yarn bundle && node server.cjs",
    "express": "node ./src/server/index.cjs",
    "build": "yarn bundle && yarn build:es5 && yarn build:esm",
    "bundle": "rollup --config rollup.config.js",
    "build:es5": "tsc -p ./tsconfig.es5.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "test:unit": "vitest ./src/tests/unit",
    "test:e2e": "playwright test",
    "fmt": "prettier ./src --write",
    "typecheck": "tsc -p ./tsconfig.json",
    "version-file": "node -p \"'export const VERSION = \\'' + require('./package.json').version + '\\';'\" > src/version.ts"
  },
  "dependencies": {
    "node-fetch": "^3.3.0",
    "node-static": "^0.7.11",
    "tslib": "^2.3.1",
    "unfetch": "^4.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.7",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "http-server": "^14.1.1",
    "isomorphic-fetch": "^3.0.0",
    "morgan": "^1.10.0",
    "prettier": "^3.2.5",
    "rollup": "^4.14.1",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  }
}
