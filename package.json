{
    "name": "optiprism_analytics_browser",
    "version": "0.0.1",
    "sideEffects": false,
    "scripts": {
        "start-test-server": "yarn bundle && node server",
        "build": "yarn bundle && yarn build:es5 && yarn build:esm",
        "bundle": "rollup --config rollup.config.js",
        "build:es5": "tsc -p ./tsconfig.es5.json",
        "build:esm": "tsc -p ./tsconfig.esm.json",
        "clean": "rimraf node_modules lib coverage",
        "fmt": "prettier ./src --write",
        "lint:eslint": "eslint '{src,test}/**/*.ts'",
        "typecheck": "tsc -p ./tsconfig.json",
        "version-file": "node -p \"'export const VERSION = \\'' + require('./package.json').version + '\\';'\" > src/version.ts"
    },
    "dependencies": {
        "node-fetch": "^3.3.0",
        "node-static": "^0.7.11",
        "tslib": "^2.3.1",
        "unfetch": "^4.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.1.0",
        "@commitlint/config-conventional": "^16.0.0",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-typescript": "^8.3.1",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.16",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest": "^25.7.0",
        "http-server": "^14.1.1",
        "husky": "^7.0.4",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^27.4.7",
        "lerna": "^4.0.0",
        "lint-staged": "^12.2.2",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.69.0",
        "rollup-plugin-execute": "^1.1.1",
        "rollup-plugin-gzip": "^3.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^27.1.3",
        "typedoc": "^0.22.13",
        "typescript": "^4.5.5"
    }
}
